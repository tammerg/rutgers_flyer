<div class="container">
  <div class="row">
    {{#each restaurant}}
      <h3>{{this.name}}:<span>{{this.cuisine}}</span></h3>
      <h4>{{this.address}} {{this.phone}}</h4>
    {{/each}}
  </div>
</div>

<div class="section no-pad-bot" id="index-banner">
  <div class="container">
    <br><br>
    <h1 class="header center orange-text">Restaurant info</h1>
    <div class="row center">
      <h5 class="header col s12 light">Here are some different restaurants around campus</h5>
    </div>
  </div>
</div>
